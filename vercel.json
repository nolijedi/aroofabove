{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/services",
      "destination": "/"
    },
    {
      "source": "/:path*",
      "destination": "/"
    }
  ]
}
